comparelmfrk; %rk dopo
close;
e_ad = abs(adams_bf-real_values);
e_mid = abs(midpoint-real_values);
e_eul = abs(euler-real_values);
e_mou = abs(adams_m-real_values);
e_trap = abs(trap-real_values);
e_sim = abs(simpson-real_values);
e_rk = abs(rk-real_values);
hold on;
left_in=1;
right_in=2500;
plot(intval(left_in:right_in),e_mid(left_in:right_in),'DisplayName','Midpoint Error');
plot(intval(left_in:right_in),e_ad(left_in:right_in),'DisplayName','Adams-BF Error');
plot(intval(left_in:right_in),e_eul(left_in:right_in),'DisplayName','Euler Error');
plot(intval(left_in:right_in),e_trap(left_in:right_in),'DisplayName','Trapezes Error');
plot(intval(left_in:right_in),e_mou(left_in:right_in),'DisplayName','Adams-M Error');
plot(intval(left_in:right_in),e_sim(left_in:right_in),'DisplayName','Simpson Error');
plot(intval(left_in:right_in),e_rk(left_in:right_in),'DisplayName','Runge Kutta Error');
legend(gca,'show','Location','northwest');