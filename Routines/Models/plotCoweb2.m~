[pnStable,snStable,dnStable,sfun,dfun] = coweb(100,0.15,10,0.10,400,10);
hold all;
x = 280:440;
sx = sfun(x);
dx = dfun(x);
plot(sx,'DisplayName','S');
plot(dx,'DisplayName','D');
pts = [300,340,380,420];
plot
d = [dfun(s(1)),dfun(s(2)),dfun(s(3))];
plot(s,d);
legend(gca,'show','Location','southeast');


